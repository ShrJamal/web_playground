{
  "name": "playground",
  "description": "Playground template to play with Vanilla Web",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Jamal .S",
    "email": "me@shrjamal.com",
    "url": "https://shrjamal.com"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
